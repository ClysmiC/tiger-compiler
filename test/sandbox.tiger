	function doubleMe (input : int) : int
	begin
		return input + input;
	end;

	function quadrupleMe (input : int) : int
	begin
		input := doubleMe(input);
		input := doubleMe(input);
		return input;
	end;

	function fillArray( firstInt : int, secondInt: int)
        begin
        	buffer[0] := firstInt;
        	buffer[1] := secondInt;
        end;
in
