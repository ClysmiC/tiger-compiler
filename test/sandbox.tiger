let
	type ArrayInt = array[100] of int;

	var myArray : ArrayInt;
	var x : int := 0;
	var y : int := 7;
	var z : int := 20;

	var loopCounter : int;

	var flute : float := 3.5;

	function doubleMe (input : int) : int
	begin
		return input + input;
	end;

	function quadrupleMe (input : int) : int
	begin
		input := doubleMe(input);
		input := doubleMe(input);
		return input;
	end;
in
	for loopCounter := 1 + 3 to 6 + 7
	do
		if (loopCounter & 1) then
			x := x + y;
			flute := x / 1.5;
		else
			x := x + z;
			flute := x / 1.5;
		endif;
	enddo;

	x := quadrupleMe(x);
	flute := flute * 2;

	printi(x);
	printf(flute);
end